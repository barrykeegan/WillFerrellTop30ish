@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
@if (Session != null && Session["userType"] != null && Session["userType"].ToString() == "staff")
{
    Response.Redirect(Url.Action("AdministratorOrders", "Order"));
}
else if (Session != null && Session["userType"] != null && Session["userType"].ToString() == "customer")
{
    Response.Redirect(Url.Action("CustomerOrders", "Order"));
}
else
{
    Response.Redirect(Url.Action("SignIn", "Customer"));
}